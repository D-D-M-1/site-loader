{
    "name": "Quick Load",
    "description": "Cycle through a list of pages quickly!!",
    "version": "0.8",
    "permissions": [
    "storage", "tabs","http://*/",
            "https://*/*"
    ],
    "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },


    "background": {
    "scripts": ["background.js"]
    },
    "manifest_version":2,
	"web_accessible_resources": [
		"toolbar.html"
  ]
}